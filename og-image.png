  <text x="300" y="330" font-family="system-ui, -apple-system, sans-serif" font-size="48" font-weight="700" text-anchor="middle" fill="white">JR</text>
  
  <!-- Main Text -->
  <text x="480" y="250" font-family="system-ui, -apple-system, sans-serif" font-size="64" font-weight="700" fill="#f5f5f5">Jack Rentschler</text>
  <text x="480" y="300" font-family="system-ui, -apple-system, sans-serif" font-size="32" font-weight="400" fill="#b9ff66">Professional Web Developer</text>
  <text x="480" y="360" font-family="system-ui, -apple-system, sans-serif" font-size="24" font-weight="400" fill="#8a8a8a">Crafting digital experiences with</text>
  <text x="480" y="390" font-family="system-ui, -apple-system, sans-serif" font-size="24" font-weight="400" fill="#8a8a8a">modern technologies</text>
  
  <!-- Domain -->
  <text x="480" y="450" font-family="system-ui, -apple-system, sans-serif" font-size="20" font-weight="500" fill="#b9ff66">jrentschler.com</text>
</svg>